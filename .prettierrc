{
  "useTabs": true,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 120,
  "bracketSpacing": true,
  "proseWrap": "always",
  "tabWidth": 4,
  "jsxBracketSameLine": false,
  "semi": true,
  "plugins": [
    "prettier-plugin-svelte"
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "svelteAllowShorthand": true
      }
    }
  ]
}
